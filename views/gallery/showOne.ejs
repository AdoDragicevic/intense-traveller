	  <div class="container-fluid" style="height: 100%;">
		  
		  <div class="row">
			  
			  <div class="col-12">
				  
				  <a class="btn btn-lg btn-outline-info float-right m-2" href="/gallery/<%= paramsId %>">Back to Gallery</a>
				  
			  </div>
			  
			  <div class="d-none d-lg-inline col-lg-1">
				  <a class="streched-link"  href="/gallery/<%= paramsId %>/<%= gallery.imgs[indxBack]._id %>">Previous</a>
			  </div>
			  
			  <div class="col-12 col-lg-10 text-center">
				  
				  
				  <img src="<%= gallery.imgs[indx].img %>">
				  
				  
			  </div>
			  
			  <div class="d-none d-lg-inline col-lg-1">
				  
				<a class="streched-link" href="/gallery/<%= paramsId %>/<%= gallery.imgs[indxNext]._id %>" style="height: 100%; width: 100%;">Next</a>
				  
				   
			  </div>
			
			  
			  <div class="col-12">
				  
				  
				  
				  
				  
				  
				
	
				
					
				  
				  
 
			  </div>
			  
			  <div class="col-12">
				  
				  <a href="/gallery/<%= paramsId %>/<%= gallery.imgs[indxBack]._id %>">Previous</a>
