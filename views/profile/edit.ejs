<%- include("../partials/header") %>


<h1>Edit <em> "<%= gallery.title %>" </em> </h1>


<form action="/profile" method="POST" enctype="multipart/form-data"> 
	<input type="text" name="gallery[title]" value="<%= gallery.title %>">
	<input type="file" id="imgUpload" name="img" accept="images/*" multiple>
	<input type="submit" value="Update">
</form>

<form action="/gallery/<%= gallery._id %>?_method=DELETE" method="POST">
	<input type="submit" value="Delete Gallery">
</form>


<%- include("../partials/header") %>