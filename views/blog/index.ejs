<%- include("../partials/header") %>

<h1><em>Intense Traveller</em></h1>

<a href="/gallery">Gallery</a>
<a href="/blog/new">Create New Blog</a>


<% blogs.forEach(function(blog){ %>
	<div>
		<h2><%= blog.title %></h2>
		<img src="<%= blog.img %>">	
		<p><%= blog.content %></p>
		<a href="/blog/<%= blog._id %>">Read Me</a>
	</div>
<% }); %>



<%- include("../partials/footer") %>